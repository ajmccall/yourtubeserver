<html>
<head></head>
<body>
	  <div>
	<h3>Accounts</h3>
	<table>
	    <tr>
	      <th>Account #</th>
	      <th>UserToken</th>
	      <th>Email</th>
	      <th>Facebook</th>
	      <th>Create</th>
	      <th>LastModified</th>
	    </tr>

	  	<% @accounts.each do |account| %>
  			<tr>
			    <td><%= account.accountNumber %></td>
			    <td><%= account.userToken %></td>
			    <td><%= account.email %></td>
			    <td><%= account.facebook %></td>
			    <td><%= account.created %></td>
			    <td><%= account.lastModified %></td>
			</tr>
		<% end %>
    </table>
</body>
</html>